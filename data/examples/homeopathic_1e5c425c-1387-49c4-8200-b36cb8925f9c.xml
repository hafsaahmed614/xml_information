         <component>
            <section ID="Lb16f832a-a88e-4613-a562-19554e60c0e6">
               <id root="29fa418a-b50a-42f8-882f-c5679447aede"/>
               <code code="34071-1" codeSystem="2.16.840.1.113883.6.1" displayName="WARNINGS SECTION"/>
               <title/>
               <text>
                  <paragraph>
                     <content styleCode="bold">WARNINGS</content>
                     <br/>
                     <content styleCode="italics">
                        <content styleCode="bold">FOR ANIMAL USE ONLY. KEEP OUT</content>
                     </content>
                     <br/>
                     <content styleCode="italics">
                        <content styleCode="bold">OF REACH OF CHILDREN. DO NOT</content>
                     </content>
                     <br/>
                     <content styleCode="italics">
                        <content styleCode="bold">USE IF TAMPER EVIDENT NECK</content>
                     </content>
                     <br/>
                     <content styleCode="italics">
                        <content styleCode="bold">SEAL IS BROKEN OR MISSING.</content>
                     </content>
                     <br/>
                     <content styleCode="italics">Safe use in pregnant animals or</content>
                     <br/>
                     <content styleCode="italics">animals intended for breeding has</content>
                     <br/>
                     <content styleCode="italics">not been proven.</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="italics"/>
                     <br/>
                     <content styleCode="italics">If animal’s condition worsens or does</content>
                     <br/>
                     <content styleCode="italics">not improve, discontinue use and</content>
                     <br/>
                     <content styleCode="italics">consult your veterinarian</content>
                  </paragraph>
               </text>
               <effectiveTime value="20220111"/>
               <component>
